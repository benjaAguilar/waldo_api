generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id            Int         @id @default(autoincrement())
  username      String?
  startDate     DateTime
  endDate       DateTime?
  time          Int?
  leaderboard   Leaderboard @relation(fields: [leaderboardId], references: [id])
  leaderboardId Int
}

model Leaderboard {
  id    Int    @id @default(autoincrement())
  name  String
  users User[]
  imageData ImageData?
}

model ImageData {
  id       Int    @id @default(autoincrement())
  x1       Int
  x2       Int
  y1       Int
  y2       Int
  width    Int
  height   Int
  imgRoute String
  name String
  leaderboard   Leaderboard @relation(fields: [leaderboardId], references: [id])
  leaderboardId Int         @unique
}
